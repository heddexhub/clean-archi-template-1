{
  "env": {
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended","plugin:@typescript-eslint/recommended"],
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  }
}
